function calculateinstal()
{
 var transaction=jQuery.trim(jQuery(".transaction").val());
 var en= jQuery.trim(jQuery(".enddate").val()); 
  var period = jQuery.trim(jQuery(".period").val()); 
if(en != ''){

var t1 = transaction.split('/');
var en1 = en.split('/');

var tdate1=new Date(t1[2],(t1[0]-1),t1[1]);
var endate1= new Date(en1[2], (en1[0]-1), en1[1]);

 var Diff=Math.ceil((endate1-tdate1)/(1000*60*60*24)); 
 
 
if(period == 'Week'){ 
  
    if(Diff < 7){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }else{
       
         if(parseInt(Diff/7) > 999 || parseInt(Diff/7)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }
         
          jQuery(".installments").val(parseInt(Diff/7)+1); 
          if(transaction!='' || en != '')
          {
              jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
           recurringtotalprocess();
          }
    }
}  

if(period == '2 weeks'){ 
  
    if(Diff < 14){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }else{
       
         if(parseInt(Diff/14) > 999 || parseInt(Diff/14)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }
         
          jQuery(".installments").val(parseInt(Diff/14)+1); 
          if(transaction!='' || en != '')
          {
              jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
           recurringtotalprocess();
          }
    }
}
if(period == 'Month'){    
   if(Diff < 30){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }else{
        
         if(parseInt(Diff/30) > 999 || parseInt(Diff/30)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }    
         jQuery(".installments").val(parseInt(Diff/30)+1); 
         if(transaction!='' || en != '')
         {
             jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
         recurringtotalprocess();
         } 
    }
}
if(period == '2 Months'){    
     if(Diff < 61){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }else{
    if(parseInt(Diff/61) > 999 || parseInt(Diff/61)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }    
         jQuery(".installments").val(parseInt(Diff/61)+1); 
         if(transaction!='' || en != '')
         { 
             jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
           recurringtotalprocess();
         }
    }
}
if(period == 'Quarter'){    
     if(Diff < 91){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }else{
    if(parseInt(Diff/91) > 999 || parseInt(Diff/91)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }    
         jQuery(".installments").val(parseInt(Diff/91)+1);
         if(transaction!='' || en != '')
         {  
             jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
         recurringtotalprocess();
         }
    }
}
if(period == '6 Months'){    
     if(Diff < 183){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }else{
    
    if(parseInt(Diff/183) > 999 || parseInt(Diff/183)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }    
         jQuery(".installments").val(parseInt(Diff/183)+1);
         if(transaction!='' || en != '')
         {
             jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
         recurringtotalprocess(); 
         }
     }
}
if(period == 'Year'){    
     if(Diff < 365){
        alert('Please select valid End Date');
        jQuery(".enddate").attr('disabled',false);  
        return false;
    }
    else{
    if(parseInt(Diff/365) > 999 || parseInt(Diff/365)< 1){
             alert('# of Installments should be greater than 1 and less than 1000');
             return false;
         }    
         jQuery(".installments").val(parseInt(Diff/365)+1);  
         if(transaction!='' || en != '')
         {
          jQuery("#light").attr('style','display:block');
         jQuery("#fade").attr('style','display:block');
         recurringtotalprocess();
         }
     }
}
} 
