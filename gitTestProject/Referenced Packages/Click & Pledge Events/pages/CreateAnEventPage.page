function CreditOptions(){
var CreditValue= document.getElementById('{!$Component.CreateAnEvent.block.SelectPayOptions.CreditCardItem.Credit_card}').checked;
//alert(CreditValue);

var temp=document.getElementById('{!$Component.CreateAnEvent.block.SelectPayOptions.CreditCardItem.CreditCardOptions}');

if(CreditValue==true){
temp.style.display='block';
}
else if(CreditValue==false){
temp.style.display='none';
}

var CustomValue= document.getElementById('{!$Component.CreateAnEvent.block.SelectPayOptions.CreditCardItem.CustomPayment}').checked;
var temp=document.getElementById('{!$Component.CreateAnEvent.block.SelectPayOptions.CreditCardItem.DisplayCustomPaymentName}');

if(CustomValue==false){

temp.style.display='none';
}
if(CustomValue==true){
temp.style.display='block';
}

var SendReceiptCheck= document.getElementById('{!$Component.CreateAnEvent.block.CustomInfo.SendReceiptItem.Eventinfo_send_receipt}').checked;
var temp1=document.getElementById('{!$Component.CreateAnEvent.block.SendReceiptSection}');
if(SendReceiptCheck==false){
temp1.style.display='none';
}

if(SendReceiptCheck==true){
temp1.style.display='block';
}

}

function popitup(url) {    
    newwindow=window.open(url,'name','height=400,width=350,scrollbars=yes');
    if (window.focus) {newwindow.focus()}
    return false;
}
function changedtohttps(url)
{
      url_1 = /^(((ht){1}(tp:[/][/]){1})|((www.){1}))[-a-zA-Z0-9@:%_\+.~#?&//=]+$/;
      if(url!="")
      {
             if(url.match(url_1))
             {
                 var re_place = url.replace('http:','https:'); 
                // var re_place=re_place.replace('force.com','secure.force.com');                   
                 $(".Eventinfo_siteurl").attr('value',re_place);
             }
      }
}
function trimvalue(value,fieldclass)
{
     var replace=value.trim();
     $("."+fieldclass).attr('value',replace);
}
