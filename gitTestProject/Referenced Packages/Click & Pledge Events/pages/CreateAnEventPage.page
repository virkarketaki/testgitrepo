              alert("Please Enter a Valid Event Name");
              $(".Eventinfo_name").focus();
              return false;
          }
          if(ev_external=="" || !ev_external.indexOf(' ',0) || !reg_text.test(ev_external))
          {
              alert("Please Enter a Valid Event Title");
              $(".Eventinfo_title").focus();
              return false;
          }
          if(!url.test(Eventinfo_siteurl) || Eventinfo_siteurl=="" || !Eventinfo_siteurl.indexOf(' ',0))
          {
             alert("Please Enter The SalesForce Public Site URL\n e.g:- https://salesforce.com");
             $(".Eventinfo_siteurl").focus();
             return false;
          }
          if(sd=="")
          {
               alert('Please enter the "Starts Date"');
               $(".Datetime_StartDT").focus();
               return false;
          }
          if(ed=="")
          {
               alert('Please enter the "Ends Date"');
               $(".Datetime_endDT").focus();
               return false;
          }
                 
          if(pd=="")
          {
                alert('Please enter the "Published Date"');
                $(".Datetime_publishdate").focus();
                return false;
          }
         
          if (Date.parse(sd) > Date.parse(ed)) {
                alert("Invalid Date Range\nStart Date cannot be after End Date!")
                return false;
           }
           if(Date.parse(pd) > Date.parse(ed))
           {   
                alert("Invalid Date Range\nPublish Date cannot be after End Date!")
                return false;               
           }
           var n = $(".actid").val();
           var reg = /^([0-9])+$/;
           if(reg.test(n)==false || n=="")
           {
                  alert('Please enter valid "C&P Account Number"');
                  $(".actid").focus();
                  return false;         
          }
          
          if(OrgInfo.length>1500){
          alert('Organization Information size should be 1500 charaters');
          $(".Eventinfo_Org_info").focus();
                  return false; 
          }
          
          if(ThanksR.length>500){
          alert('Receipt Thank You Message size should be 500 charaters');
          $(".Eventinfo_Thanks").focus();
                  return false; 
          }
          
           if(RTC.length>1500){
          alert('Receipt Terms & Conditions size should be 1500 charaters');
          $(".Eventinfo_Receipt_Terms_Condition").focus();
                  return false; 
          }
          
          
          if(parseInt(TotalInventory)<parseInt(InventorySold) || parseInt(TotalInventory)==0){
          
          alert("Invalid Maximum Capacity");
          return false;
          }
          var Credit=$(".Credit_card").val();
          var Creditcard =$(".Credit_card").is(':checked');
          var Visa=$(".Visa_card").is(':checked');
          var master=$(".Master_card").is(':checked');
          var Discover=$(".Discover").is(':checked');
          var AmericanEx=$(".AmericanEx").is(':checked');
          var e_check=$(".e_check").is(':checked');
          var InVoice=$(".InVoice").is(':checked');
          var PurchaseOrder=$(".PurchaseOrder").is(':checked');
          
         
          var acknowledgement=$(".acknowledgement").is(':checked');
          var showterm = $(".showterm").is(':checked');
          //alert(Custompayment);
          
          if(Creditcard==false){
          jQuery(".Visa_card").val('');
          jQuery(".Master_card").val('');
          jQuery(".Discover").val('');
          jQuery(".AmericanEx").val('');
          }
          
          if(Custompayment==true && (Custompaymentname=="" || !Custompaymentname.indexOf(' ',0) || !reg_text.test(Custompaymentname))){
          alert("Please Enter Custom Payment Name");
          $(".CustomPayment_Name").focus();
          return false;
          }
          
          if(Custompaymentname.length>50){
          alert("Custom Payment Name Should Be 50 characters Only.");
          $(".CustomPayment_Name").focus();
          return false;
          }
         
          if(Creditcard ==false && e_check==false && InVoice==false && PurchaseOrder==false && Custompayment==false){
