        <url>//Query Submission and Offer

{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot;)};

var result = sforce.connection.query(
&quot;Select Name, Id from McaApp__Submission__c where McaApp__Opportunity__c =&apos;{!Opportunity.Id}&apos;&quot;);

var resultOffer = sforce.connection.query(
&quot;Select Name, Id from McaApp__Offer__c where McaApp__Opportunity__c =&apos;{!Opportunity.Id}&apos;&quot;);

submission= result.getArray(&quot;records&quot;); 
offer= resultOffer .getArray(&quot;records&quot;);


if({!ISBLANK( Opportunity.AccountId )}){ 
  alert(&apos;Please associate Opportunity with Account to create Contract&apos;);
}
else if(submission.length &gt; 0 || offer.length &gt; 0){
window.open(&quot;/apex/McaApp__CreateContract?id={!Opportunity.Id}&amp;stage={!Opportunity.StageName}&amp;accId={!Opportunity.AccountId}&amp;name={!Opportunity.Name}&quot;,&quot;_self&quot;);

}else{
   alert(&quot;{!$Label.mcaapp__CreateContract_NoSubmissionNOfferErr}&quot;);
}</url>
