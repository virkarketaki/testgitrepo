    <fields>
        <fullName>Age__c</fullName>
        <description>Compute the Age based on entered DOB</description>
        <externalId>false</externalId>
        <formula>IF( MONTH( Date_of_Birth__c )  &lt;=  MONTH( TODAY() ) , 
 IF( MONTH( Date_of_Birth__c )  =  MONTH( TODAY() )  ,
 IF( DAY( Date_of_Birth__c )  &gt;  DAY( TODAY() ) ,
 YEAR( TODAY() )  -  YEAR(  Date_of_Birth__c  ) - 1,
 YEAR( TODAY() )  -  YEAR(  Date_of_Birth__c  ) ) , 
 YEAR( TODAY() )  -  YEAR(  Date_of_Birth__c  )) ,
 YEAR( TODAY() )  -  YEAR(  Date_of_Birth__c  ) - 1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
